              xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
             xmlns:local="clr-namespace:ChatAppRealTime" StartupUri="MainWindow.xaml?isRegister=false"><!--MainWindow.xaml?isRegister=false-->
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ui:ThemesDictionary Theme="Dark" />
